# Note: This file is managed by Puppet.

# /etc/default/storm

JAVA_LIBRARY_PATH="/usr/share/java:/usr/local/lib:/opt/local/lib:/usr/lib"
STORM_USER="storm"
STORM_HOME="<%= storm_home %>"
STORM_LIB="$STORM_HOME/lib"
STORM_JAR="$STORM_HOME/storm-*.jar"
STORM_CONF="/etc/storm"
STORM_CLASSPATH="$(echo $STORM_JAR | tr ' ' ':'):$(echo $STORM_LIB/*.jar | tr ' ' ':'):$(echo $STORM_CONF | tr ' ' ':'):$STORM_HOME"
STORM_OPTIONS=""
